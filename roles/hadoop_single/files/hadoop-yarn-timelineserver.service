[Unit]
Description=Hadoop YARN Timeline Server (ATS v1)
After=network.target
Wants=network.target

[Service]
Type=forking
User=hadoop
Group=hadoop
EnvironmentFile=/etc/default/hadoop
ExecStart=/usr/lib/hadoop-yarn/bin/yarn --daemon start timelineserver
ExecStop=/usr/lib/hadoop-yarn/bin/yarn --daemon stop timelineserver
Restart=on-failure
RestartSec=5

[Install]
WantedBy=multi-user.target
